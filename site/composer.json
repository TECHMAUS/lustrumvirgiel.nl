{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.6.7",
        "type": "wordpress-muplugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
        },
        "require": {
          "philippbaschke/acf-pro-installer": "^1.0",
          "composer/installers": "^1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "enviragallery/envira-instagram",
        "version": "1.3.0",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/envira-instagram-1.3.0.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "enviragallery/envira-social",
        "version": "1.4.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/envira-social-1.4.1.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "enviragallery/envira-dynamic",
        "version": "1.4.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/envira-dynamic-1.4.1.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "enviragallery/envira-videos",
        "version": "1.3.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/envira-videos-1.3.1.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "enviragallery/envira-pagination",
        "version": "1.5.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/envira-pagination-1.5.3.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "enviragallery/envira-gallery",
        "version": "1.7.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/envira-gallery-1.7.1.zip"
        },
        "require": {
          "enviragallery/envira-instagram": "^1.3.0",
          "enviragallery/envira-social": "^1.4.1",
          "enviragallery/envira-dynamic": "^1.4.1",
          "enviragallery/envira-videos": "^1.3.1",
          "enviragallery/envira-pagination": "^1.5.3",
          "composer/installers": "^1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ithemes/ithemes-security-pro",
        "version": "4.7.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://s3-eu-west-1.amazonaws.com/composer.lustrumvirgiel.nl/ithemes-security-pro-4.7.3.zip"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.4",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.9.1",
    "oscarotero/env": "^1.1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/wp-polls": "^2.73",
    "roots/soil": "^3.7",
    "ithemes/ithemes-security-pro": "^4.7.3",
    "wpackagist-plugin/amazon-s3-and-cloudfront": "^1.2",
    "wpackagist-plugin/amazon-web-services": "^1.0",
    "wpackagist-plugin/tinymce-advanced": "^4.6",
    "wpackagist-plugin/onesignal-free-web-push-notifications": "^1.16",
    "enviragallery/envira-gallery": "^1.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "vendor/bin/phpcs"
    ]
  }
}
